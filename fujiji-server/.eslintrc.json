{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jasmine": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "global-require": "off",
    "eqeqeq": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
  }
}